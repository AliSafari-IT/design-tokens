{
  "name": "@asafarim/design-tokens",
  "version": "0.4.3",
  "private": false,
  "description": "ASafariM design tokens (CSS variables) with multi-theme support and 30 variable fonts (100-900).",
  "license": "MIT",
  "author": "ASafariM",
  "repository": {
    "type": "git",
    "url": "https://github.com/AliSafari-IT/design-tokens.git"
  },
  "keywords": [
    "design-tokens",
    "css-variables",
    "theming",
    "react",
    "CSS",
    "themes",
    "asafarim"
  ],
  "type": "module",
  "style": "css/index.css",
  "files": [
    "css",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": [
    "css/*.css"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./css": {
      "style": "./css/index.css",
      "default": "./css/index.css"
    },
    "./css/index.css": "./css/index.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "demo": "pnpm --filter design-tokens-demo dev",
    "build": "tsup",
    "build:demo": "cd demo && pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "bugs": {
    "url": "https://github.com/AliSafari-IT/design-tokens/issues"
  },
  "homepage": "https://github.com/AliSafari-IT/design-tokens#readme"
}